{
  "name": "sewer-inspection-viewer",
  "version": "1.0.0",
  "description": "Full-stack sewer inspection PDF parser and viewer",
  "scripts": {
    "install-all": "cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm start",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "build:railway": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "postinstall": "npm run install-all",
    "verify-deployment": "node verify-deployment.js"
  },
  "keywords": ["sewer", "inspection", "pdf", "viewer", "wincan"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=16.x",
    "npm": ">=8.x"
  }
}
